<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="au theme template">
    <meta name="author" content="Hau Nguyen">
    <meta name="keywords" content="au theme template">

    <!-- Title Page-->
    <title>Doctor Dashboard</title>

    {{>dashboard-styles}}
    <style>
        .height-200 {
            height: 200px;
        }

        .header__navbar ul li a {
            color: #000 !important;
        }


        .overview__inner i {
            font-size: 30px !important;
            position: relative;
            top: -30px;
        }

        .overview__inner h2 {
            padding-left: 10px !important;
            font-size: 30px !important;
        }

        .overview__inner h2:first-child {
            font-size: 30px !important;
            padding-left: 0px !important;
            position: relative;
            left: 12px;
            padding-left: 0px;
        }
    </style>
</head>

<body class="animsition">
    <div class="page-wrapper">
        <!-- HEADER DESKTOP-->
        <header class="header-desktop3 d-none d-lg-block" style="background-color: white; height: 100px;">
            <div class="section__content section__content--p35">
                <div class="header3-wrap">
                    <div class="header__logo">
                        <a href="/">
                            <img src="../../public/img/logo.png" alt="medfork" style="max-width: 230px;" />
                        </a>
                    </div>
                    <div class="header__navbar" style="top: 25px;">
                        <ul class="list-unstyled">
                            <li class="has-sub">
                                <a href="http://127.0.0.1:8081/dashboard-doctor/">
                                    <i class="fas fa-tachometer-alt"></i>Dashboard
                                    <span class="bot-line"></span>
                                </a>
                            </li>
                            <li>
                                <a href="http://127.0.0.1:8081/dashboard-doctor/about">
                                    <i class="fa fa-user" aria-hidden="true"></i>
                                    <span class="bot-line"></span>About
                                </a>
                            </li>
                            <li>
                                <a href="http://127.0.0.1:8081/dashboard-doctor/key-generation">
                                    <i class="fa fa-list" aria-hidden="true"></i>
                                    <span class="bot-line"></span>Patient Registration</a>
                            </li>
                            <li>
                                <a href="http://127.0.0.1:8081/dashboard-doctor/medical-details">
                                    <i class="fa fa-calendar-o" aria-hidden="true"></i>
                                    <span class="bot-line"></span>Medical Details</a>
                            </li>
                            <li>
                                <a href="http://127.0.0.1:8081/dashboard-doctor/shared-data">
                                    <i class="fa fa-phone" aria-hidden="true"></i>
                                    <span class="bot-line"></span>Shared Records</a>
                            </li>

                        </ul>
                    </div>
                    <div class="header__tool">
                        <div class="account-wrap">
                            <div class="account-item account-item--style2 clearfix js-item-menu">
                                <div class="image">
                                    <img src="../../public/dashboard/images/icon/user.png" alt="user" />
                                </div>
                                <div class="content">
                                    <a class="js-acc-btn" style="color: #333;" href="#">{{doctorName}}</a>
                                </div>
                                <div class="account-dropdown js-dropdown">
                                    <div class="info clearfix">
                                        <div class="image">
                                            <a href="#">
                                                <img src="../../public/dashboard/images/icon/user.png" alt="user" />
                                            </a>
                                        </div>
                                        <div class="content">
                                            <h5 class="name">
                                                <a href="#">{{doctorName}}</a>
                                            </h5>
                                            <span class="email">{{doctorRegNo}}</span>
                                        </div>
                                    </div>
                                    <div class="account-dropdown__body">
                                        <div class="account-dropdown__item">
                                            <a href="about">
                                                <i class="zmdi zmdi-account"></i>Account</a>
                                        </div>
                                    </div>
                                    <div class="account-dropdown__footer">
                                        <a href="/">
                                            <i class="zmdi zmdi-power"></i>Logout</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <!-- END HEADER DESKTOP-->

        <!-- PAGE CONTENT-->
        <div class="page-content--bgf7">
            <!-- BREADCRUMB-->
            <section class="au-breadcrumb2">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="au-breadcrumb-content">
                                <div class="au-breadcrumb-left">
                                    <span class="au-breadcrumb-span">You are here:</span>
                                    <ul class="list-unstyled list-inline au-breadcrumb__list">
                                        <li class="list-inline-item active">
                                            <a href="/">Home</a>
                                        </li>
                                        <li class="list-inline-item seprate">
                                            <span>/</span>
                                        </li>
                                        <li class="list-inline-item">Dashboard</li>
                                    </ul>
                                </div>
                                <form class="au-form-icon--sm" method="post">
                                    <input class="au-input--w300 au-input--style2" type="text" id="search"
                                        placeholder="Search for data &amp; reports...">
                                    <button class="au-btn--submit2" type="submit" id="submit">
                                        <i class="zmdi zmdi-search"></i>
                                    </button>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!-- END BREADCRUMB-->

            <!-- WELCOME-->
            <section class="welcome p-t-10">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <h1 class="title-4">Easy Access
                                <!----------- Send patient name from server  --------------->
                                <span></span>
                            </h1>
                            <hr class="line-seprate">
                        </div>
                    </div>
                </div>
            </section>
            <!-- END WELCOME-->

            <!-- GENERAL SECTION-->
            <section class="statistic statistic2">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-6 col-lg-3">
                            <div class="overview-item overview-item--c1" id="about-btn">
                                <a href="/dashboard-doctor/about">
                                    <div class="overview__inner">
                                        <div class="overview-box clearfix">
                                            <div class="icon">
                                                <i class="zmdi zmdi-account-o" style="top: -3px;"></i>
                                            </div>
                                            <div class="text">
                                                <h2>About</h2>
                                            </div>
                                        </div>
                                        <div class="overview-chart">
                                        </div>
                                    </div>
                                </a>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-3">
                            <div class="overview-item overview-item--c2" id="prescription-btn">
                                <a href="/dashboard-doctor/key-generation">
                                    <div class="overview__inner">
                                        <div class="overview-box clearfix">
                                            <div class="icon">
                                                <i class="fa fa-list" aria-hidden="true"></i>
                                                <div class="text">
                                                    <h2>Patient</h2>
                                                    <h2>Registration</h2>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="overview-chart" style="position: relative; height: 82px;">
                                        </div>
                                    </div>
                                </a>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-3">
                            <div class="overview-item overview-item--c3" id="appoinment-btn">
                                <a href="/dashboard-doctor/medical-details">
                                    <div class="overview__inner">
                                        <div class="overview-box clearfix">
                                            <div class="icon">
                                                <i class="zmdi zmdi-calendar-note"
                                                    style="position: relative; top: -32px;"> </i>
                                                <div class="text">
                                                    <h2>Medical</h2>
                                                    <h2>Details</h2>
                                                    <span></span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="overview-chart" style="position: relative; height: 82px;">
                                        </div>
                                    </div>
                                </a>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-3">
                            <div class="overview-item overview-item--c4" id="other-details-btn">
                                <a href="/dashboard-doctor/shared-data">

                                    <div class="overview__inner">
                                        <div class="overview-box clearfix">
                                            <div class="icon">
                                                <i class="fa fa-share-alt" aria-hidden="true"></i>
                                                <div class="text">
                                                    <h2>Shared</h2>
                                                    <h2>Records</h2>
                                                    <span></span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="overview-chart" style="position: relative; height: 82px;">
                                        </div>
                                    </div>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!-- END GENERAL SECTION-->

            <!-- END PREVIOUS SECTION -->

